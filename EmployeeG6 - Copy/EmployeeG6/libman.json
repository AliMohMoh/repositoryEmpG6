{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "library": "bootstrap-icons@1.7.2",
      "destination": "wwwroot/lib/bootstrap-icons/"
    },
    {
      "library": "bootswatch@5.1.3",
      "destination": "wwwroot/lib/bootswatch/",
      "files": [
        "cerulean/bootstrap.min.css"
      ]
    },
    {
      "library": "limonte-sweetalert2@11.3.6",
      "destination": "wwwroot/lib/limonte-sweetalert2/",
      "files": [
        "sweetalert2.min.js",
        "sweetalert2.min.css"
      ]
    },
    {
      "library": "bootstrap-rtl@3.4.0",
      "destination": "wwwroot/lib/bootstrap-rtl/",
      "files": [
        "css/bootstrap-rtl.min.css"
      ]
    }
  ]
}